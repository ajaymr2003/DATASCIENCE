   "source": [
    "import numpy as np\n",
    "X=np.array([[1, 2],[3, 4],[4,3],[3,9]])\n",
    "U,S,VT=np.linalg.svd(X)\n",
    "n_comp=2\n",
    "X_reconst=np.dot(U[:,:n_comp],np.dot(np.diag(S[:n_comp]),VT[:n_comp,:]))\n",
    "print(\"Original matrix : \\n\",X)\n",
    "print(\"Reconstructed :\\n\",X_reconst)"
   ]
